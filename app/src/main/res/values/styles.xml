<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#e1bb7b</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowBackground">@color/putih</item>
        <item name="android:colorBackground">@color/putih</item>
        <item name="android:textColorPrimary">@color/putih</item>
    </style>

    <style name="ToolbarTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="android:textColorPrimary">@color/putih</item>
        <item name="android:textColorSecondary">@color/Abu</item>
        <item name="android:textColor">@color/putih</item>
    </style>

    <style name="TitleColorOne">
        <item name="android:textColor">@color/hitam</item>
    </style>

</resources>
